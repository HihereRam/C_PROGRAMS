//Multiplication table -pr3
#include<stdio.h>
int main(){
 int n, i;
 printf("Enter the number :\n");
 scanf("%d",&n);
 printf("Multiplcation table :\n");
 for(int i=1; i<=10; i++){
     printf("%d x %d=" "%d\n",n,i,n*i);
     }
  return 0;
}
//OUTPUT
Enter the number :
4
Multiplcation table :
4 x 1=4
4 x 2=8
4 x 3=12
4 x 4=16
4 x 5=20
4 x 6=24
4 x 7=28
4 x 8=32
4 x 9=36
4 x 10=40
