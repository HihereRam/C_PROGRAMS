//Traffic light system-pr1
#include<stdio.h>
int main(){
    int light;
    
    printf("Traffic light system \n");
    printf("1.Red\n2.Green\n3.Yellow\n Enter your Choice : \n");
    scanf("%d", &light);
    
    switch(light){
        case 1:
            printf("STOP");
            break;
        case 2 :
            printf("GO");
            break;
        case 3 :
            printf("READY");
            break;
        default :
          printf("Invalid");
          break;
    }
    return 0;
}

//OUTPUT
Traffic light system 
1.Red
2.Green
3.Yellow
Enter your Choice :1
STOP
