//Sum of the natural numbers-pr4
#include<stdio.h>
int main(){
    int n,i,sum=0;
    
    printf("Enter the number:");
    scanf("%d",&n);
    
    for(int i=1;i<=n;i++){
        sum += i;

    }
    printf("Sum of all the values %d = %d", n, sum);
      return 0;   
}
//OUTPUT
Enter the number:5
Sum of all the values 5 = 15
